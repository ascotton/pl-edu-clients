{
  "name": "edu-clients",
  "version": "0.1.5",
  "description": "Presence Learning Customer App",
  "repository": {
    "type": "git",
    "url": "https://github.com/presencelearning/edu-clients.git"
  },
  "scripts": {
    "copy-repos": "gulp copy_repos",
    "ng": "ng",
    "env-dev": "node ./node-scripts/set-env --envkey=dev",
    "env-local": "node ./node-scripts/set-env --envkey=local",
    "env-localdev": "node ./node-scripts/set-env --envkey=localdev",
    "env-test": "node ./node-scripts/set-env --envkey=test",
    "env-stag": "node ./node-scripts/set-env --envkey=stag",
    "env-stagdev": "node ./node-scripts/set-env --envkey=stagdev",
    "env-live": "node ./node-scripts/set-env --envkey=live",
    "env-jb": "node ./node-scripts/set-env --envkey=jb",
    "env-k8s": "node ./node-scripts/set-env --envkey=k8s",
    "setup": "npm run env-dev && npm run env-local && npm run copy-repos",
    "setup-stag": "npm run env-dev && npm run env-stagdev && npm run copy-repos",
    "setup-localdev": "npm run env-dev && npm run env-localdev && npm run copy-repos",
    "start": "npm run setup && ng serve --configuration=local",
    "ipad-dev": "npm start",
    "start-localdev": "npm run setup-localdev && ng serve --configuration=localdev --deploy-url=/c/",
    "start-stag": "npm run setup-stag && ng serve --configuration=stagdev --deploy-url=/c/",
    "start-aot": "npm run setup && ng serve --aot --configuration=local --deployUrl=/c",
    "prebuild": "gulp prebuild",
    "svg": "gulp bundle_svg",
    "build": "npm run setup && ng build --configuration=local --deploy-url=/c/",
    "--ng-build": "ng build --configuration=$NG_ENV",
    "ng-build": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --configuration=$NG_ENV --stats-json",
    "build-test": "npm run setup && npm run env-test && NG_ENV=test npm run ng-build",
    "build-stag": "npm run setup && npm run env-stag && NG_ENV=stag npm run ng-build",
    "build-k8s": "npm run setup && npm run env-k8s && NG_ENV=k8s npm run ng-build",
    "build-live": "npm run setup && npm run env-live && NG_ENV=production npm run ng-build",
    "ci-setup": "npm run env-local && npm run env-dev",
    "ci-build": "npm run build",
    "ci-unit-tests": "npm run test-unit-ci",
    "lite": "lite-server",
    "test": "ng test",
    "coverage": "ng test --watch=false --codeCoverage --reporters=coverage-istanbul",
    "lint": "ng lint",
    "lint-ts": "tslint --config tslint.json 'src/**/*.ts' --exclude 'src/lib-components/**/*.ts' --format stylish",
    "lint-js": "eslint -c .eslintrc 'cypress/**/*.js'",
    "validate-gql-queries": "node ./node-scripts/validate-graphql-schemas",
    "compodoc": "compodoc -p src/tsconfig.app.json",
    "cy-help": "open https://github.com/presencelearning/edu-clients/blob/dev/CYPRESS_SCRIPTS_README.md",
    "cy-list-env": "node cypress-scripts/print-env-vars",
    "cy-install": "cd cypress-scripts && npm i",
    "cy-seed": "cypress-scripts/cy-seed.sh",
    "cy-after-seed": "cypress-scripts/cy-after-seed.sh",
    "cy-foundry": "cypress-scripts/foundry-yaml.sh",
    "cy-multi-seed": "cypress-scripts/cy-multi-seed.sh",
    "cy-ingest": "cypress-scripts/ingest-users.sh",
    "cy-login": "npm run cy-list-env; time (node cypress-scripts/login-and-save-status.js) && echo",
    "cy-ingest-login": "cypress-scripts/cy-ingest-login.sh",
    "cy-tester": "rm -f mochawesome.json && rm -f cypress/results/*.json && time (cypress run) && npx mochawesome-merge \"cypress/results/*.json\" > mochawesome.json && npx marge mochawesome.json && echo",
    "cy-suite": "rm -f mochawesome.json && rm -f cypress/results/*.json && time (cypress-scripts/run-suite.sh) && npx mochawesome-merge \"cypress/results/*.json\" > mochawesome.json && npx marge mochawesome.json && echo",
    "cy-basic-seed": "export CY_SUITE=basic-smoke; npm run cy-seed",
    "cy-basic-ingest-login": "export CY_SUITE=basic-smoke; npm run cy-ingest-login",
    "cy-basic-multi-seed": "export CY_SUITE=basic-smoke; npm run cy-multi-seed",
    "cy-basic-smoke": "export CY_SUITE=basic-smoke; npm run cy-login && time (cypress run --record --reporter mochawesome --spec cypress/integration/pl/suites/basic-smoke/**/*.spec.js)",
    "qa-basic-smoke": "export CY_SUITE=basic-smoke; npm run cy-basic-seed; npm run cy-after-seed; npm run cy-regression",
    "cy-single-test": "npm run cy-login && cypress-scripts/cy-single-test.sh",
    "cy-basic-single": "export CY_SUITE=basic-smoke; npm run cy-login && cypress-scripts/cy-single-test.sh",
    "cy-regression-seed": "export CY_SUITE=regression; npm run cy-seed",
    "cy-regression-ingest-login": "export CY_SUITE=regression; npm run cy-ingest-login",
    "cy-regression-multi-seed": "export CY_SUITE=regression; npm run cy-multi-seed",
    "cy-regression": "export CY_SUITE=regression; npm run cy-login && time (cypress run --record --reporter mochawesome --spec cypress/integration/pl/suites/regression/**/*.spec.js)",
    "qa-regression": "export CY_SUITE=regression; npm run cy-regression-seed; npm run cy-after-seed; npm run cy-regression",
    "cy-regression-single": "export CY_SUITE=regression; npm run cy-login && cypress-scripts/cy-single-test.sh",
    "cy-dashboard": "open -a \"Google Chrome\" https://dashboard.cypress.io/projects/cvb7dt/runs",
    "cy-reporter": "if [ \"$(command -v open)\" ];then npm run __cy-report-open;else npm run __cy-report-list;fi",
    "cy-test": "time (npm run cy-login && npm run cy-tester) && npm run cy-reporter && echo",
    "cy-all": "time (npm run cy-seed && && cy-after-seed && npm run cy-test) && echo",
    "cy-users": "cypress-scripts/show-users.sh",
    "cy-fixtures": "cypress-scripts/show-fixtures.sh",
    "cy-open": "cypress open -c testFiles=pl/**/*",
    "cy-open-suites": "cypress open -c testFiles=pl/suites/**/*",
    "cy-open-basic-smoke": "cypress open -c testFiles=pl/suites/basic-smoke/**/*",
    "cy-open-regression": "cypress open -c testFiles=pl/suites/regression/**/*",
    "pw-test": "npx playwright test $@",
    "pw-test-d": "npx playwright test --debug $@",
    "pw-test-h": "npx playwright test --headed $@",
    "pw-test-r": "npx playwright show-report",
    "pw-room-test": "cd playwright && node room-test $@",
    "__cy-report-open": "open -a \"Google Chrome\" ./cypress/videos/pl && open ./mochawesome-report/mochawesome.html",
    "__cy-report-list": "echo \"./cypress/videos/pl\" && echo \"./mochawesome-report/mochawesome.html\""
  },
  "license": "MIT",
